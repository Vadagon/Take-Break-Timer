{
    "name": "Timender - AI productivity Manager",
    "version": "4",
    "author": "Vadagon",
    "manifest_version": 2,
    "description": "Smartly reminds you when to take a break.",
    "permissions": [
        "storage",
        "notifications",
        "background",
        "idle",
        "power"
    ],
    "browser_action": {
        "default_icon": {
            "16": "images/16.png",
            "19": "images/19.png",
            "20": "images/20.png",
            "32": "images/32.png",
            "38": "images/38.png",
            "40": "images/40.png",
            "128": "images/pause.png"
        },
        "default_title": "Timender",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["custom/js/plugins.js", "background.js"],
        "persistent": true
    },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "64": "images/64.png",
        "128": "images/pause.png"
    },
    "web_accessible_resources": [
        "images/*",
        "custom/*",
        "sounds/*"
    ],
    "options_page": "options.html",
    "offline_enabled": true,
    "minimum_chrome_version": "50"
}